{
  "name": "electron-valtio",
  "version": "0.0.13",
  "description": "",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "clean": "rm -rf dist",
    "pub": "gulp"
  },
  "exports": {
    "./package.json": "./package.json",
    "./main": {
      "types": "./main.d.ts",
      "module": "./esm/main.js",
      "import": "./esm/main.js",
      "default": "./main.js"
    },
    "./preload": {
      "types": "./preload.d.ts",
      "module": "./esm/preload.js",
      "import": "./esm/preload.js",
      "default": "./preload.js"
    },
    "./renderer": {
      "types": "./renderer.d.ts",
      "module": "./esm/renderer.js",
      "import": "./esm/renderer.js",
      "default": "./renderer.js"
    },
    "./setup": {
      "types": "./setup.d.ts",
      "module": "./esm/setup.js",
      "import": "./esm/setup.js",
      "default": "./setup.js"
    },
    "./sync": {
      "types": "./types/sync.d.ts",
      "module": "./esm/sync.js",
      "import": "./esm/sync.js",
      "default": "./cjs/sync.js"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "electron": "^12.0.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.3",
    "gulp-shell": "^0.8.0",
    "typescript": "^4.2.3",
    "valtio": "^1.0.0"
  }
}
